{
  "name": "express-admin",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "nodemon --exec babel-node ./bin/www",
    "dev": "./start-dev.sh",
    "prod": "./start-prod.sh",
    "start-project": "babel-node scripts/start-project.js",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testNamePattern=unit",
    "test:integration": "jest --testNamePattern=integration",
    "test:manual": "node scripts/run-manual-tests.js",
    "test:run": "node scripts/run-tests.js",
    "test:run:watch": "node scripts/run-tests.js --watch",
    "test:run:coverage": "node scripts/run-tests.js --coverage",
    "test:run:ci": "node scripts/run-tests.js --ci",
    "test:openrouter": "babel-node tests/integration/test-openrouter.js",
    "test:statistics": "babel-node scripts/test-statistics.js",
    "create-admin": "babel-node scripts/create-admin.js",
    "setup-env": "babel-node scripts/setup-env.js",
    "setup-ollama": "babel-node scripts/setup-ollama.js",
    "optimize-ollama": "babel-node scripts/optimize-ollama.js",
    "reduce-lag": "babel-node scripts/reduce-lag.js",
    "fix-recurring-tasks": "babel-node scripts/fix-recurring-tasks.js",
    "test-recurring-status": "babel-node scripts/test-recurring-status.js",
    "generate-fake-stats": "node scripts/generate-fake-statistics.js",
    "docs:generate": "apidoc -i src/ -o docs/api/",
    "docs:serve": "apidoc -i src/ -o docs/api/ && serve docs/api/",
    "api:generate": "node scripts/generate-api-docs.js"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.0",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.6",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^6.14.1",
    "helmet": "^7.1.0",
    "http-errors": "~1.6.3",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "nodemailer": "^7.0.3",
    "nodemon": "^3.1.10",
    "ollama": "^0.4.7",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0",
    "regenerator-runtime": "^0.14.1",
    "undici": "^5.28.4",
    "web-push": "^3.6.7",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/node": "^7.16.8",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.16.11",
    "apidoc": "^1.2.0",
    "babel-jest": "^29.7.0",
    "cors": "^2.8.5",
    "jest": "^29.7.0",
    "mongoose": "^8.16.0",
    "supertest": "^6.3.3"
  }
}
